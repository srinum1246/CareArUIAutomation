<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="PortFolio Management">
    
  <listeners>
	<listener class-name="listener.MyTestNGListener"></listener>
  </listeners>
  <test name="Session Activity">
    <classes>
      <class name="testCases.analystRoleCases.analystRolePages">
       <methods>
           <include name="verifyUSTextEditableORNot"/>
           <include name="verifyEmailTextEditableORNot"/>
           <include name="verifyNameTextEditableORNot"/>
           <include name="verifyJobDescriptionTextEditableORNot"/>
           <include name="VerifyPrimaryPhoneNumberCountryCode9digitNumber"/>
           <include name="VerifyPrimaryPhoneNumber5Numbers"/>
           <include name="VerifyPrimaryPhoneNumber10DigitsStartWithZero"/>
           <include name="VerifyPrimaryPhoneNumber20Digits"/>
           <include name="VerifyPrimaryPhoneNumber10DigitsStartsWithOne"/>
           <include name="VerifyPrimaryPhoneNumberWithString"/>
           <include name="VerifyPrimaryPhoneNumber10DigitsCountryCode0WithPlus"/>
           <include name="VerifyPrimaryPhoneNumber10DigitsWithoutCountryCode"/>
           <include name="VerifyPrimaryPhoneNumber18DigitsWithPlus"/>
           <include name="VerifyPrimaryPhoneNumber10DigitsWithPlusCountryCode"/>
       </methods>
      </class>
    </classes>
  </test>
